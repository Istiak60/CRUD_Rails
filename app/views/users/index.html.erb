<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<table>
  <tr>
    <th>Email</th>
    <th>Id</th>
    <th>Show</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
   <% @users.each do |user| %>
<tr>
    <td><%= user.email%></td>
    <td><%= user.id%></td>
   
             
<td> <%= link_to "Show ", user_path(user.id), class:"nav-link" %></td>
     <td>
          <%= link_to "Edit ", edit_user_registration_path, class:"nav-link" %>
        </td>
    <td> <%= button_to "Delete", user_path(user),
                                    method: :delete
                                   %></td>
    
  </tr>
   <% end %>
</table> 

 
  